{
    "require": {
        "ext-pdo": "*",
        "doctrine/dbal": "^3.2",
        "php-di/php-di": "^6.3",
        "selective/basepath": "^2.1",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
    "psr-4": {
        "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
